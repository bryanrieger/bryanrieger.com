{%- for preload in site.preload %}
    <link rel="preload" as="{{ preload.as }}" href="{{ preload.src }}" {% if preload.type %}type="{{ preload.type }}"{% endif %} {% if preload.crossorigin %}{{ preload.crossorigin }}{% endif %}>
{%- endfor %}
{%- getBundle "html", "<link>" %}
<link rel="icon" href="{{ site.icon.ico }}" sizes="any">
<link rel="icon" type="image/png" href="{{ site.icon.png }}">
<link rel="icon" type="image/svg+xml" href="{{ site.icon.svg }}">
<link rel="mask-icon" href="{{ site.icon.mask }}" color="{{ site.icon.color }}">
<link rel="apple-touch-icon" href="{{ site.icon.touch }}" sizes="192x192">
<link rel="author" href="{{ site.humans }}">
<link rel="sitemap" href="{{ site.sitemap }}">
<link rel="manifest" href="{{ site.manifest }}">
<link rel="canonical" href="https://{{ site.domain }}">
<link rel="alternate" href="https://{{ site.domain }}{{ site.feed.json }}" type="application/json">
<link rel="alternate" href="https://{{ site.domain }}{{ site.feed.xml }}" type="application/rss+xml">
<link rel="stylesheet" href="/assets/styles/default.css" media="screen">